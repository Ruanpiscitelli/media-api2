# Configurações da API
API_VERSION=v2
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=true

# Segurança
SECRET_KEY=test-secret-key
JWT_ALGORITHM=HS512
JWT_EXPIRE_MINUTES=60
CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# APIs Externas
CIVITAI_API_KEY=test-api-key
CIVITAI_BASE_URL=https://civitai.com/api

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=test-password

# RabbitMQ
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672

# GPU
CUDA_VISIBLE_DEVICES=0
PYTORCH_CUDA_ALLOC_CONF=max_split_size_mb:512

# Diretórios
MEDIA_DIR=./tests/test_data/media
TEMP_DIR=./tests/test_data/media/temp
LOG_DIR=./tests/test_data/logs
MODELS_DIR=./tests/test_data/models
LORA_DIR=./tests/test_data/models/lora
CHECKPOINTS_DIR=./tests/test_data/models/checkpoints

# Monitoramento
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# ComfyUI
COMFY_HOST=localhost
COMFY_PORT=8188
COMFY_API_KEY=test-comfy-key

# Cache
CACHE_TTL=3600
MAX_CACHE_SIZE=1GB

# Rate Limiting
RATE_LIMIT_DEFAULT=500
RATE_LIMIT_COMFY=50
RATE_LIMIT_IMAGE=100
RATE_LIMIT_VIDEO=20 